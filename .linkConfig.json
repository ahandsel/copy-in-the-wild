{
  "projectBaseUrl": "${workspaceFolder}",
  "ignorePatterns": [
    { "pattern": "^https://github.com/" },
    { "pattern": "^(?!/public/.*\\.(png|jpg|jpeg|gif|svg|ico)).*$" }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
